<BuilderCall id="bc120"><BuilderDefID>com.bowstreet.builders.webapp.MethodBuilder</BuilderDefID>
<Messages />
<Inputs><Input name="BuilderCallEnabled">true</Input>
<Input name="ReturnType">void</Input>
<Input name="Body">{
 //Obtener resultados del dataservice
 IXml results = webAppAccess.getDataServiceParameterXml("GetTransacciones","GetTransaccionesOperation","results","operation1Response");
 //Obtener inputs del data service
 IXml operation1IXml = webAppAccess.getDataServiceParameterXml("GetTransacciones","GetTransaccionesOperation","inputs","operation1");
 
 //Variables 
 double sumaDebito=0;
 double sumaCredito =0;
 int cantidadDebito=0;
 int cantidadCredito=0;
 String moneda = webAppAccess.getVariables().getString("Moneda");
 
 //Obtener datos de los inputs
  int idCuenta =Integer.parseInt(operation1IXml.getText("id_cuenta"));
  String numeroCuenta =( idCuenta+10000)+"";
  String idTipoTransaccion =  operation1IXml.getText("id_tipo_transaccion");
  String tipoTransaccion = idTipoTransaccion.equals("-1") ? "Todas" : (idTipoTransaccion.equals("1") ? "Credito" : "Debito");
  //Obtener datos de los results
  IXml child = results.getFirstChildElement();
  String monedaTransaccion="NoSelec";
 while(child!=null){
       String origenNumCuenta=child.getText("num_cuenta_origen");
       String debito =child.getText("debito");
       String credito =child.getText("credito");
       monedaTransaccion = child.getText("moneda"); 
       if((debito ==null )|| (credito ==null)){
       		break;
       }
       if(!debito.equals("")){
           if(monedaTransaccion.equals(moneda)){
        		sumaDebito +=Double.parseDouble(debito);
           }
          else  if(moneda.equals("Dolares")){
               sumaDebito +=Double.parseDouble(debito)/500;
           }
           else{
               sumaDebito +=Double.parseDouble(debito)*500;
           }
           cantidadDebito++;
       }
        if(!credito.equals("")){
	         sumaCredito +=Double.parseDouble(credito);
          	 cantidadCredito++;
       }
      //Siguiente transaccion 
 	  child = child.getNextSiblingElement();
 }

//Setear variables
webAppAccess.getVariables().setString("CantidadEgresoSinFomato",sumaCredito+"");
webAppAccess.getVariables().setString("CantidadIngresoSinFormato", sumaDebito+"");
String sumaCreditoStr=" ";
String sumaDebitoStr =" ";
sumaCreditoStr =(String)webAppAccess.callMethod("formatNumber", sumaCredito+"", moneda);
sumaDebitoStr =(String)webAppAccess.callMethod("formatNumber", sumaDebito+"", moneda);
webAppAccess.getVariables().setString("CantidadEgreso", cantidadCredito+"");
webAppAccess.getVariables().setString("CantidadIngreso", cantidadDebito+"");
webAppAccess.getVariables().setString("Egreso", sumaCreditoStr);
webAppAccess.getVariables().setString("Ingreso", sumaDebitoStr);
webAppAccess.getVariables().setString("NumeroCuenta",numeroCuenta);
webAppAccess.getVariables().setString("IdTipoTransaccion",tipoTransaccion);

//Imprimir variables
System.out.println(webAppAccess.getVariables().getString("CantidadEgreso"));
System.out.println(webAppAccess.getVariables().getString("CantidadIngreso"));
System.out.println(webAppAccess.getVariables().getString("Egreso"));
System.out.println(webAppAccess.getVariables().getString("Ingreso"));
System.out.println(webAppAccess.getVariables().getString("NumeroCuenta"));
System.out.println(webAppAccess.getVariables().getString("IdTipoTransaccion"));

}</Input>
