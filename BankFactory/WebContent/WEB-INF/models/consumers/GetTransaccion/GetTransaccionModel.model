    if(!moneda.equals("NoSeleccionado")){
	    System.out.println("numero = "+numero + "moneda =" +moneda);
	    if(!numero.equals("")){
		     double num = Double.parseDouble(numero);
		    DecimalFormat formateador = new DecimalFormat("###,###,###.##");
		    numeroStr =  formateador.format (num);
			if(moneda.equals("Dolares"))
				numeroStr="$"+numeroStr;
			else if(moneda.equals("Colones"))
			    numeroStr="¬¢"+numeroStr;
			else
				return numeroStr;
	    }
	    return numeroStr;
    }
    else
    	return numeroStr;
}</Input>
