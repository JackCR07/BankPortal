    <ViewAndForm_InputPageStyleSheetOverride>/factory/html_templates/mobile/mobile_table_slide.css</ViewAndForm_InputPageStyleSheetOverride>
    <DSUI_StyleSheetOverride>/factory/html_templates/mobile/mobile_table_slide.css</DSUI_StyleSheetOverride>
    <DSUI_CreatePageStyleSheetOverride>/factory/html_templates/mobile/mobile_table_slide.css</DSUI_CreatePageStyleSheetOverride>
    <DSUI_UpdatePageStyleSheetOverride>/factory/html_templates/mobile/mobile_table_slide.css</DSUI_UpdatePageStyleSheetOverride>
    <DSUI_DetailsPageStyleSheetOverride>/factory/html_templates/mobile/mobile_table_slide.css</DSUI_DetailsPageStyleSheetOverride>
